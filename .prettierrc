{
    "useTabs": false,
    "singleQuote": true,
    "trailingComma": "none",
    "printWidth": 100,
    "tabWidth": 4,
    "semi": true,
    "quoteProps": "consistent",
    "bracketSameLine": true,
    "bracketSpacing": true,
    "arrowParens": "always",
    "overrides": [
        {
            "files": "*.js",
            "options": {
                "parser": "babel"
            }
        }
    ]
}
